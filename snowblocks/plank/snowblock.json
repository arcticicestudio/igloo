[
  {
    "clean": ["$XDG_CONFIG_HOME/plank", "$XDG_DATA_HOME/plank"]
  },
  {
    "link": {
      "$XDG_CONFIG_HOME/plank/polarbear/launchers": {
        "create": true,
        "force": true,
        "hosts": {
          "polarbear": "launchers/polarbear"
        }
      },
      "$XDG_DATA_HOME/plank/themes/polarbear": {
        "create": true,
        "force": true,
        "hosts": {
          "polarbear": "themes/polarbear"
        }
      }
    }
  }
]
