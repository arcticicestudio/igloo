[
  {
    "clean": ["~"]
  },
  {
    "link": {
      "~/.gitattributes": {
        "hosts": {
          "iceowl": "gitattributes",
          "igloo": "gitattributes"
        }
      },
      "~/.git-commit-msg": null,
      "~/.gitconfig": null,
      "~/.gitconfig-auth": {
        "hosts": {
          "iceowl": "gitconfig-auth",
          "igloo": "gitconfig-auth"
        }
      },
      "~/.gitconfig-auth-svengreb": {
        "hosts": {
          "iceowl": "gitconfig-auth-svengreb",
          "igloo": "gitconfig-auth-svengreb"
        }
      },
      "~/.gitconfig-auth-job": {
        "hosts": {
          "iceowl": "gitconfig-auth-job.igloocrypt",
          "igloo": "gitconfig-auth-job.igloocrypt"
        }
      },
      "~/.gitignore": null,
      "~/.gitconfig-macos-mergetool-exit-code": {
        "hosts": {
          "iceowl": "includes/gitconfig-macos-mergetool-exit-code"
        }
      }
    }
  }
]
